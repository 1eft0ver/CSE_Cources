package myjava.homework;

// Represents a deposit ATM transaction

public class Deposit extends Transaction{
	private int  amount;
	public Deposit() {
		amount = 0;
	}
	public Deposit(int money) {
		amount = money;
	}
	public void execute(BankDatabase db) {
		db.credit(this.getAccountNumber(), amount);
	}
}
